     52          static LCD_COLOR _Index2Color_444_12_1(unsigned Index) {
     53            unsigned int r, g, b;
     54            /* Seperate the color masks */
     55            r = (Index >> 12) & 0xf;
     56            g = (Index >>  8) & 0xf;
     57            b = (Index >>  4) & 0xf;
     58            /* Convert the color masks */
     59            r = r * 17;
     60            g = g * 17;
     61            b = b * 17;
     62            return r + (g << 8) + (((U32)b) << 16);
