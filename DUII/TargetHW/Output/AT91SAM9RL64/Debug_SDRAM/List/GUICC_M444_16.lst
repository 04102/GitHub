     52          static LCD_COLOR _Index2Color_M444_16(unsigned Index) {
     53            unsigned r, g, b;
     54            /* Separate the color masks */
     55            b = (Index >>  1) & 0xf;
     56            g = (Index >>  6) & 0xf;
     57            r = (Index >> 11) & 0xf;
     58            /* Convert the color masks */
     59            r = r * 17;
     60            g = g * 17;
     61            b = b * 17;
     62            return r + (g << 8) + (((U32)b) << 16);
